                </Grid>
                <Grid Row="2" Column="1" HorizontalAlignment="Left">
                    <TextBox Name="fUtil_Nom"  Width="220"/>
                </Grid>
                <Grid Row="3" Column="1" HorizontalAlignment="Left">
                    <TextBox Name="fUtil_Nomconx"  Width="90"/>
                </Grid>
                <Grid Row="4" Column="1" HorizontalAlignment="Left">
