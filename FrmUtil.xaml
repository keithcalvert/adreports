                    <Label Content="Description"/>
                </Grid>
                <Grid Row="8">
                    <Label Content="Bureau"/>
                </Grid>
                <Grid Row="9">
                    <Label Content="Numéro de téléphone"/>
                </Grid>
                <Grid Row="10">
                    <Label Content="E-mail"/>
                </Grid>
                <Grid Row="11">
                    <Label Content="Page Web"/>
                </Grid>
                </Grid>
            </TabItem>
        </TabControl>
        <StackPanel  Grid.Row="12" Grid.Column="2" Orientation="Horizontal">
            <StackPanel.Resources>
                <Style TargetType="{x:Type Button}">
                    <Setter Property="Margin" Value="0,10,10,10"/>
                </Style>
            </StackPanel.Resources>
            <Button Name="fUtil_btnQuit" Content="Quitter"/>
        </StackPanel>
    </StackPanel>
</Window>
