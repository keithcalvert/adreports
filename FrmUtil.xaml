                </Grid>
                <Grid Row="8" Column="1" HorizontalAlignment="Left">
                    <TextBox Name="fUtil_Bureau"  Width="120"/>
                </Grid>
                <Grid Row="9" Column="1" HorizontalAlignment="Left">
                    <TextBox Name="fUtil_NumTel"  Width="120"/>
                </Grid>
                <Grid Row="10" Column="1" HorizontalAlignment="Left">
                    <TextBox Name="fUtil_Email"  Width="160"/>
                </Grid>
                <Grid Row="11" Column="1" HorizontalAlignment="Left">
                    <TextBox Name="fUtil_PageWeb"  Width="200"/>
                </Grid>
                </Grid>
            </TabItem>

            <TabItem Header="Compte">
